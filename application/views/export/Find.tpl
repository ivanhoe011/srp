<h2>Monitoreo de Planillas</h2>
<br><br>
<form action="{url action=excel}" method="post" class="validate">
	<table class="center"">
		<caption>{$l10n->_('Fechas de Búsqueda')}</caption>
		<tr>
    		<th>{$l10n->_('Inicio')}</th>
			<td><input name="startp" type="text" size="12" id="startp" class="datePicker" value="{$startDate}"/></td> 
			<th>{$l10n->_('Fin')}</th>   
    		<td><input name="endp" type="text" size="12" id="endp" class="datePicker" value="{$endDate}"/></td>   
    		<tfoot>
	        	<tr>
    	        	<td colspan="4">
    	        		<input type="submit" value="{$l10n->_('Expotar')}" />
    	        		<!--<a href="{$baseUrl}/export/excel/start/{$startDate}/end/{$endDate}/export/1" color:"blue">
    						{$l10n->_('Exportar a Excel')}
    					</a>-->
    	        	</td>
        		</tr>
    		</tfoot>
		</tr>
	</table>
</form>
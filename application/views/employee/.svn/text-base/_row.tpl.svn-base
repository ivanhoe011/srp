_row.tpl

<tr>
    <td>{$employee->getIdEmployee()}</td>
    <td>{$employee->getIdUser()}</td>
    <td>{$employee->getIdDepartment()}</td>
    <td>{$employee->getType()}</td>
    <td>{$employee->getBeginningDate()}</td>
